var pageNumber = 1;
var minPageNumber = 1;
var maxPageNumber = ;

function nextPage() {
	pageNumber = pageNumber + 1;
	if (pageNumber > maxPageNumber) {
		pageNumber = maxPageNumber;
	}
	var x = (pageNumber - 1) * 280.0;
	document.getElementById("book").setAttribute("viewBox", "" + x + " 0 280.0 190.0");
}

function previousPage() {
	pageNumber = pageNumber - 1;
	if (pageNumber < minPageNumber) {
		pageNumber = minPageNumber;
	}
	var x = (pageNumber - 1) * 280.0;
	document.getElementById("book").setAttribute("viewBox", "" + x + " 0 280.0 190.0");
}

function firstPage() {
	pageNumber = minPageNumber;
	var x = (pageNumber - 1) * 280.0;
	document.getElementById("book").setAttribute("viewBox", "" + x + " 0 280.0 190.0");
}

function lastPage() {
	pageNumber = maxPageNumber;
	var x = (pageNumber - 1) * 280.0;
	document.getElementById("book").setAttribute("viewBox", "" + x + " 0 280.0 190.0");
}

function printUntilEnd() {
	for (var i = pageNumber; i <= maxPageNumber; i++) {
		window.print();
		nextPage();
	}
}

function printAll() {
	firstPage();
	printUntilEnd();
}

var key = document.getElementsByClassName("key");

function hideKey() {
	for (var i = 0; i < key.length; i++) {
		key[i].style.visibility = "hidden";
	}
}

function showKey() {
	for (var i = 0; i < key.length; i++) {
		key[i].style.visibility = "visible";
	}
}

var keyVisible = 1;

function toggleKey() {
	if (keyVisible == 1) {
		hideKey();
		keyVisible = 0;
	} else {
		showKey();
		keyVisible = 1;
	}
}
